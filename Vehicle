class Vehicle {
    String brand;
    void start() {
        System.out.println(brand + " started");
    }
}

class Car extends Vehicle {
    int doors;
    void displayCar() {
        System.out.println("Car with " + doors + " doors");
    }
}

class Bike extends Vehicle {
    boolean hasGear;
    void displayBike() {
        System.out.println("Bike with gear: " + hasGear);
    }
}

public class VehicleTest {
    public static void main(String[] args) {
        Car c = new Car();
        c.brand = "Toyota";
        c.doors = 4;
        c.start();
        c.displayCar();

        Bike b = new Bike();
        b.brand = "Yamaha";
        b.hasGear = true;
        b.start();
        b.displayBike();
    }
}
